{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${env:JAVA_HOME}/include",
                "${env:JAVA_HOME}/include/darwin",
                "${workspaceFolder}/**",
                "${default}"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "Linux",
            "includePath": [
                "${env:JAVA_HOME}/include",
                "${env:JAVA_HOME}/include/linux",
                "${workspaceFolder}/**",
                "${default}"
            ],
            "defines": [],
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "Win32",
            "includePath": [
                "${env:JAVA_HOME}/include",
                "${env:JAVA_HOME}/include/win32",
                "${workspaceFolder}/**",
                "${default}"
            ],
            "intelliSenseMode": "msvc-x64",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}
